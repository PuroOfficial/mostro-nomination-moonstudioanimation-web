{
  "version": 2,
  "builds": [
    {
      "src": "nomination/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/mostro-nomination/(.*)",
      "dest": "/mostro-nomination/$1"
    },
    {
      "src": "/api/(.*)", 
      "dest": "/mostro-nomination/api/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html"
    }
  ]
}